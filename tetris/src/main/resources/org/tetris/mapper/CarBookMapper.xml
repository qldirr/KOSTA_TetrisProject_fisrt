<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.tetris.mapper.CarBookMapper">

	<select id="getListResCar" resultType="org.tetris.domain.CarBookVO">
		select * from carbook
	</select>

	<insert id="insertResCar" parameterType="org.tetris.domain.CarBookVO">
		insert into carbook (cb_num)
		values (#{cb_num})
	</insert>
	
	<select id="readResCar" resultType="org.tetris.domain.CarBookVO">
	select * from carbook where e_id =#{e_id}
	</select>



</mapper>
  
  